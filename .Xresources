!================================ XEyes related ================================
XEyes.geometry:                                 75x75-2130+50
XEyes*foreground:                               #000000
XEyes*background:                               #ffffff
XEyes*outline:                                  #101010
XEyes*center:                                   #ffffff

!================================ XMenu related ================================
xmenu.font:                                     FiraMono Nerd Font:size=14:antialias=true
xmenu.background:                               #5f005f
xmenu.foreground:                               #bbbbbb
xmenu.selbackground:                            #8700af
xmenu.selforeground:                            #eeeeee
xmenu.border:                                   #8700af
xmenu.separator:                                #af87d7
xmenu.gap:                                      10
xmenu.width:                                    100
xmenu.height:                                   48
xmenu.borderWidth:                              1
xmenu.separatorWidth:                           30

!================================ XClock related ===============================
.XClock.geometry:                               75x75-2130+150
.XClock.Clock.analog:                           true
.XClock.Clock.strftime:                         (%A)\040%F\040%T
! .XClock.Clock.update:                         1
! .XClock.Clock.render:                         false
.XClock.Clock.font:                             -sony-fixed-*-*-*-*-24-*-*-*-*-*-*-*
.XClock.Clock.background:                       #ffffff
.XClock.Clock.foreground:                       #000000

!================================ Xmessage related =============================
.Xmessage.form.message.scrollVertical:          never
.Xmessage.form.message.scrollHorizontal:        never
! .Xmessage.form.message.background:              black
! .Xmessage.geometry:                             -0-0
! .Xmessage.form.background:                      black
! .Xmessage.form.Command.font:                    -sony-fixed-*-*-*-*-24-*-*-*-*-*-*-*
! .Xmessage.form.message.font:                    -sony-fixed-*-*-*-*-24-*-*-*-*-*-*-*
.Xmessage*font:                                 -sony-fixed-*-*-*-*-24-*-*-*-*-*-*-*
! .Xmessage.form.Command.foreground:              #9d9d9d
! .Xmessage.form.Command.background:              black
.Xmessage.form.Command.shapeStyle:              Rectangle

